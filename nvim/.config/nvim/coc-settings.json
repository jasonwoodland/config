{
  "list.insertMappings": {
    "<C-n>": "normal:j",
    "<C-p>": "normal:k",
    "<C-s>": "action:split",
    "<C-v>": "action:vsplit",
    "<C-t>": "action:tabe"
  },
  "list.normalMappings": {
    "<C-j>": "normal:j",
    "<C-k>": "normal:k",
    "<C-n>": "normal:j",
    "<C-p>": "normal:k",
    "<C-s>": "action:split",
    "<C-v>": "action:vsplit",
    "<C-t>": "action:tabe"
  },
  "list.source.files.args": ["--hidden", "--files"],
  "list.source.files.excludePatterns": ["**/.git/**", "**/__generated__/**"],
  "list.source.mru.ignoreGitIgnore": true,
  "list.source.grep.command": "rg",
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "tsserver.maxTsServerMemory": 2048,
  "diagnostic.enableHighlightLineNumber": false,
  "git.addedSign.text": "üÆà",
  "git.changedSign.text": "üÆà",
  "git.removedSign.text": "‚ñÇ",
  "git.topRemovedSign.text": "üÆÇ",
  "git.addGBlameToVirtualText": true,
  "diagnostic.warningSign": "ÔÅ±",
  "diagnostic.infoSign": "ÔÅö",
  "diagnostic.errorSign": "ÔÅ™",
  "diagnostic.hintSign": "ÔÅ©",
  "diagnostic.messageTarget": "float",
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnTypeFiletypes": [
    "json",
    "jsonc",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "coc.preferences.formatOnSaveFiletypes": [
    "json",
    "jsonc",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "suggest.removeDuplicateItems": true,
  "typescript.preferences.quoteStyle": "single",
  "javascript.preferences.quoteStyle": "single",
  "eslint.autoFixOnSave": true,
  "explorer.width": 35,
  "explorer.icon.enableNerdfont": true,
  "explorer.openAction.strategy": "select",
  "explorer.keyMappings.global": {
    "v": "open:vsplit"
  },
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "javascript.jsx": ["javascript"],
    "typescriptreact": ["typescript"],
    "typescript.tsx": ["typescript"]
  },
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    },
    "graphql": {
      "command": "graphql-lsp",
      "args": ["server", "-m", "stream"],
      "filetypes": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "graphql"
      ]
    }
  },
  "translator.engines": ["google"],
  "translator.toLang": "en"
}
